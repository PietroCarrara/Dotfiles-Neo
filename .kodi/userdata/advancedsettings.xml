<advancedsettings version="1.0">

	<tvshowmatching action="append">
		<!-- As a last resource, try finding a number -->
		<regexp>[\._ \-]([0-9]+)</regexp>
	</tvshowmatching>

	<videolibrary>
		<cleanonupdate>true</cleanonupdate>
	</videolibrary>

	<videoscanner>
		<ignoreerrors>true</ignoreerrors>
	</videoscanner>

</advancedsettings>
